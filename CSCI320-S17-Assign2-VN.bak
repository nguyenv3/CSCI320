#lang Scheme
(define (make_list_of_size n e)
  (cond ((= n 0) '())
        ((= n 1) (cons e '()))
        (else (make_list_of_size (- n 1)(cons e e))
         )
    )
  )

(define (zeros list)
  (cond
    ((null? list) 0)
    ((= (car list) 0 ) (+ 1 (zeros (cdr list))))
    (else (zeros (cdr list)))
    )
  )

(define (power a b)
  (define (innerPower a b result)
    (if (zero? b) result
        (innerPower (* a a)
               (quotient b 2)
               (if (odd? b) (* result a) result))))
  (innerPower a b 1)
 )

(define (remove list1 param) 
( cond
    ((null? list1) '())
    ((equal? (car list1) param) (remove (cdr list1) param)) 
    (else (cons (car list1) (remove (cdr list1) param)))
))

(define largest
(lambda (x)
  (let ((y (car x)))
    (if (null? (cdr x)) y
      (let ((m (largest(cdr x))))
           (if (> m y) m y)
        )
      )
    )
  )  
  )