#lang racket
(define (length list)
  (cond ((null? list) 0)
        (else(+ 1 (length (cdr list))))
        )
  )

(define (permutation? list1 list2)
  (cond ((null? list1) (if(null? list2) #t #f) )
        
        )
  )

(define (remove list1 param) 
 (cond
    ((null? list1) '())
    ((equal? (car list1) param) (remove (cdr list1) param)) 
    (else (cons (car list1) (remove (cdr list1) param)))
   )
 )